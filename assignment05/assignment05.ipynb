    "class MinHeap:\n",
    "    def __init__(self):\n",
    "        self._A = []\n",
    "\n",
    "    def insert(self, x):\n",
    "        self._A.append(x)\n",
    "        self._percolateUp(len(self._A) - 1)\n",
    "\n",
    "    def _percolateUp(self, i):\n",
    "        parent = (i - 1) // 2\n",
    "        if i > 0 and self._A[i] < self._A[parent]:  # 작은 값이 위로\n",
    "            self._A[i], self._A[parent] = self._A[parent], self._A[i]\n",
    "            self._percolateUp(parent)\n",
    "\n",
    "    def deleteMin(self):\n",
    "        if self.isEmpty():\n",
    "            return None\n",
    "        min_val = self._A[0]\n",
    "        self._A[0] = self._A.pop()\n",
    "        self._percolateDown(0)\n",
    "        return min_val\n",
    "\n",
    "    def _percolateDown(self, i):\n",
    "        left = 2 * i + 1\n",
    "        right = 2 * i + 2\n",
    "        smallest = i\n",
    "\n",
    "        if left < len(self._A) and self._A[left] < self._A[smallest]:\n",
    "            smallest = left\n",
    "        if right < len(self._A) and self._A[right] < self._A[smallest]:\n",
    "            smallest = right\n",
    "\n",
    "        if smallest != i:\n",
    "            self._A[i], self._A[smallest] = self._A[smallest], self._A[i]\n",
    "            self._percolateDown(smallest)\n",
    "\n",
    "    def isEmpty(self):\n",
    "        return len(self._A) == 0\n",
    "\n",
    "    def size(self):\n",
    "        return len(self._A)\n",
    "\n",
    "    def min(self):\n",
    "        if self._A:\n",
    "            return self._A[0]\n",
    "        return None\n",
    "\n",
    "\n",
    "\n",
    "class KthLargest:\n",
    "    def __init__(self, k: int, nums: list[int]):\n",
    "        self.k = k\n",
    "        self.min_heap = MinHeap()\n",
    "\n",
    "        for num in nums:\n",
    "            self.add(num)\n",
    "\n",
    "    def add(self, val: int) -> int:\n",
    "        self.min_heap.insert(val)\n",
    "        if self.min_heap.size() > self.k:\n",
    "            self.min_heap.deleteMin()\n",
    "        return self.min_heap.min()"
